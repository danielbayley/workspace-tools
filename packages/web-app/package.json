{
  "name": "@lerepo/web-app",
  "version": "0.2.1",
  "publishConfig": {
    "access": "public",
    "directory": "package"
  },
  "description": "Companion web-app for @lerepo/cli.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lerepo/workspace-tools.git"
  },
  "engines": {
    "node": ">=12.18.3",
    "yarn": ">=2"
  },
  "scripts": {
    "_r:prepare": "yarn clean && yarn build && yarn pack && jaguar -x package.tgz && rimraf package.tgz",
    "build": "yarn webpack:quiet",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist build lib package package.tgz *.tsbuildinfo",
    "format": "yarn g:format",
    "format:all": "yarn g:format:all",
    "format:list": "yarn g:format:list",
    "lint": "yarn g:lint",
    "lint:fix": "run-lint --fix",
    "lint:staged": "yarn g:lint:staged",
    "open": "open-cli http://localhost:3000 -- firefox -devtools",
    "start": "concurrently \"npm:wds:mon\" \"npm:open\"",
    "storybook": "start-storybook -p 6006",
    "t:check": "tsc --noEmit -p tsconfig.json",
    "t:types": "yarn run t:check",
    "t:watch": "yarn run t:check --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "wds": "cross-env NODE_ENV=development webpack-dev-server",
    "wds:mon": "nodemon --config .nodemon-wds.json",
    "webpack": "cross-env NODE_ENV=production webpack --progress",
    "webpack:analyze": "cross-env NODE_ENV=production WITH_BUNDLE_ANALYZER=true webpack",
    "webpack:quiet": "cross-env NODE_ENV=production webpack --no-stats"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-force-graph-2d": "^1.23.11",
    "react-router-dom": "^6.4.1",
    "require-from-string": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.0",
    "@mdx-js/react": "^2.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "babel-loader": "^8.2.5",
    "babel-plugin-console-source": "^2.0.5",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-define": "^2.1.0",
    "browserslist": "^4.21.4",
    "can-ndjson-stream": "^1.0.2",
    "caniuse-lite": "^1.0.30001410",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.25.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "jaguar": "^6.0.1",
    "jest": "29.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.20",
    "open-cli": "^7.0.1",
    "pnp-webpack-plugin": "^1.7.0",
    "postcss": "8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "prettier-plugin-package": "^1.3.0",
    "react": "^17",
    "react-dom": "^17",
    "react-is": "^17",
    "react-refresh": "^0.14.0",
    "regenerator-runtime": "^0.13.9",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "4.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "browser": "./dist/index.html",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
